# RealDebrid API token (required)
RD_TOKEN=

# Timezone (optional, defaults to UTC if unset)
# Examples: "America/New_York", "Europe/London", "Australia/Sydney"
TZ=

# User ID for file permissions (optional, defaults to 1500)
PUID=1500

# Group ID for file permissions (optional, defaults to 1500)
PGID=1500